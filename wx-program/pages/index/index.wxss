/*  加载更多   */
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}
.weui-loadmore {
  width: 65%;
  margin: 1.5em auto;
  line-height: 1.6em;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}

.shadow{
    width:100%;
    height:100%;
    position:fixed;
    top:0;
    left:0;
    background:#000;
    z-index:9000;
    opacity:0.7;
}
.alertImg{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 9999;
  width: 15rem;
  height:11.5rem;
}
/* overflow:hidden;text-overflow:ellipsis;white-space:nowrap; */
.colonel>view>view{
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  display: inline-block;
  vertical-align: middle;
}
.colonel>view>view:nth-child(1){
  width: 50%;
  }
  .colonel>view>view:nth-child(2){
  width: 20%;
  }
   .colonel>view>view:nth-child(3){
     width:100rpx;
     height: 30rpx;
     background-color: #ff004a;
     border-radius: 15rpx;
     font-size: 18rpx;
     color: #fff;
     display: inline-flex;
     align-items: center;
     justify-content: center;
     vertical-align: middle;
  }