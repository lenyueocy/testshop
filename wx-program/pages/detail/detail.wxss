}
.share{
  height: 220rpx;
}
.wx-box,.code-box{
  background-color:#fff;
  height: 200rpx;
  float: left;
  width: 50%;
  overflow: hidden;
  text-align:center;
  position: relative;
  margin-bottom: 20rpx;
  border:0;
}
.wx-box button{
  background-color: #fff;
  border:0;
  outline: none;

}
button::after { 
border: none; 
}
.wx-box button .index-tx{
  font-size: 28rpx;
  color: #999999;
  margin-top: 130rpx;
}

.index-tx{
  font-size: 28rpx;
  color: #999999;
  margin-top: 145rpx;
}

.wx-box button image,.code-box image{
  width: 120rpx!important;
  height: 120rpx!important;
  position: absolute;
  left: 50%;
  top: 20rpx;
  transform: translate(-50%);
}

