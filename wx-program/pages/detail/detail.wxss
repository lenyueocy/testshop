page{
  padding-bottom: 110rpx;
  /* padding: 0 20rpx; */
  box-sizing: border-box;
  background-color: #efefef;
  overflow-x: hidden;
  width: 100%;
  /* overflow: hidden; */
  margin:0 auto;
}
.item-img{
  width: 100%;
  height: 400rpx;
  position: relative
}
.price>text:first-of-type{
  font-size: 20rpx;
}
.old-price{
  font-size: 30rpx;
  text-decoration: line-through;
  position: absolute;
  left: 44%;
  top: 23rpx;
}
.new-price>text:last-of-type{
  font-size: 58rpx;
  font-weight: bold;
}
.item-num{
  position: absolute;
  right: 1rpx;
  margin-left: 30rpx;
  line-height: 1.5;
}
.item-num view ,.item-num text{
  font-size: 25rpx;
  color: #887A5A;
}
.top-price{
  /* background-color: #FDF36D; */
  height: 100rpx;
  position: relative;
  background-position: -30% 0%;
  background: url("data:image/jpg;base64,");
  background-size: 105%;
}
.price{
  float: left;
  height: 83rpx;
  line-height: 83rpx;
  color: #fff!important;
}
.product-name{
  font-size: 30rpx;
  font-weight: bold;
  padding-left: 0;
  background-color: #fff;
}

.attention{
  padding: 10rpx 15rpx;
  color: #fff;
  border-radius: 10rpx;
  background-color: #FFB10E;
  position: absolute;
  right:0rpx;
  top: 0；
}

.product-des{
  min-height:70rpx;
