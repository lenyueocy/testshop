 /* pages/about/about.wxss */
 .logo{
   width: 200rpx;
   height: 200rpx;
   margin: 20rpx auto;
   border-radius: 50%;
   overflow: hidden;
 }
 .item{
   padding: 20rpx;
   color: #868FD4;
   font-weight: bold;
 }
 .telBtn{
  background: -webkit-linear-gradient(left, #BCA1FF , #7D87FF); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #BCA1FF, #7D87FF); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #BCA1FF, #7D87FF); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #BCA1FF , #7D87FF); /* 标准的语法 */
  width: 690rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  text-align: center;
  margin: 20rpx auto;
  color: #D81E06;
  font-weight: bold;
 }
 .tel{
   display: inline-block;
   width: 30rpx;
   height: 30rpx;
   margin-right: 20rpx;
   position: relative;
   top: 5rpx;
 }